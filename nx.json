{
  "npmScope": "nx-dapp",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "shared-connections-data-access-connections": {
      "tags": []
    },
    "shared-connections-data-access-endpoints": {
      "tags": []
    },
    "shared-connections-data-access-tokens": {
      "tags": []
    },
    "shared-connections-ui-dropdown": {
      "tags": []
    },
    "shared-wallets-ui-dropdown": {
      "tags": []
    },
    "web-client": {
      "tags": []
    },
    "web-client-e2e": {
      "tags": [],
      "implicitDependencies": ["web-client"]
    }
  }
}
