{
  "version": 2,
  "projects": {
    "shared-connections-data-access-connections": {
      "projectType": "library",
      "root": "libs/shared/connections/data-access/connections",
      "sourceRoot": "libs/shared/connections/data-access/connections/src",
      "prefix": "nx-dapp",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/connections/data-access/connections"
          ],
          "options": {
            "jestConfig": "libs/shared/connections/data-access/connections/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/connections/data-access/connections/src/**/*.ts",
              "libs/shared/connections/data-access/connections/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-connections-data-access-endpoints": {
      "projectType": "library",
      "root": "libs/shared/connections/data-access/endpoints",
      "sourceRoot": "libs/shared/connections/data-access/endpoints/src",
      "prefix": "nx-dapp",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/connections/data-access/endpoints"],
          "options": {
            "jestConfig": "libs/shared/connections/data-access/endpoints/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/connections/data-access/endpoints/src/**/*.ts",
              "libs/shared/connections/data-access/endpoints/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-connections-data-access-tokens": {
      "projectType": "library",
      "root": "libs/shared/connections/data-access/tokens",
      "sourceRoot": "libs/shared/connections/data-access/tokens/src",
      "prefix": "nx-dapp",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/connections/data-access/tokens"],
          "options": {
            "jestConfig": "libs/shared/connections/data-access/tokens/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/connections/data-access/tokens/src/**/*.ts",
              "libs/shared/connections/data-access/tokens/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-connections-ui-dropdown": {
      "projectType": "library",
      "root": "libs/shared/connections/ui/dropdown",
      "sourceRoot": "libs/shared/connections/ui/dropdown/src",
      "prefix": "nx-dapp",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/connections/ui/dropdown"],
          "options": {
            "jestConfig": "libs/shared/connections/ui/dropdown/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/connections/ui/dropdown/src/**/*.ts",
              "libs/shared/connections/ui/dropdown/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-wallets-ui-dropdown": {
      "projectType": "library",
      "root": "libs/shared/wallets/ui/dropdown",
      "sourceRoot": "libs/shared/wallets/ui/dropdown/src",
      "prefix": "nx-dapp",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/wallets/ui/dropdown"],
          "options": {
            "jestConfig": "libs/shared/wallets/ui/dropdown/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/wallets/ui/dropdown/src/**/*.ts",
              "libs/shared/wallets/ui/dropdown/src/**/*.html"
            ]
          }
        }
      }
    },
    "web-client": {
      "projectType": "application",
      "root": "apps/web-client",
      "sourceRoot": "apps/web-client/src",
      "prefix": "nx-dapp",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/web-client",
            "index": "apps/web-client/src/index.html",
            "main": "apps/web-client/src/main.ts",
            "polyfills": "apps/web-client/src/polyfills.ts",
            "tsConfig": "apps/web-client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/web-client/src/favicon.ico",
              "apps/web-client/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/web-client/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/web-client/src/environments/environment.ts",
                  "with": "apps/web-client/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "web-client:build:production"
            },
            "development": {
              "browserTarget": "web-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-client:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web-client/src/**/*.ts",
              "apps/web-client/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/web-client"],
          "options": {
            "jestConfig": "apps/web-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-client-e2e": {
      "root": "apps/web-client-e2e",
      "sourceRoot": "apps/web-client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web-client-e2e/cypress.json",
            "tsConfig": "apps/web-client-e2e/tsconfig.e2e.json",
            "devServerTarget": "web-client:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web-client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/web-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "web-client"
}
